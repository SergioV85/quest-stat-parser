{
  "name": "quest-stat-parser-server",
  "version": "0.1.0",
  "description": "A server statistic parser for Quest.Ua games",
  "main": "server.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "compile": "tsc --declaration",
    "deploy": "serverless deploy",
    "test": "ts-mocha -p tsconfig.json src/**/*.test.ts",
    "test:coverage": "nyc ts-mocha -p tsconfig.json src/**/*.test.ts",
    "test:ci": "npm run test:coverage && npm run prepare-code-coverage-report",
    "start:local": "serverless offline",
    "start:server": "http-server",
    "prettier": "prettier --config ./.prettierrc --list-different \"./src/**/*.{ts,js,json}\"",
    "precommit": "npm run prettier && npm run lint",
    "lint": "./node_modules/.bin/tslint -c tslint.json -p tsconfig.json",
    "prepare-code-coverage-report": "replace '...*src' 'SF:./src' coverage/lcov.info"
  },
  "author": "Serhii Vozniak",
  "license": "MIT",
  "dependencies": {
    "aws-lambda": "^1.0.5",
    "async": "^2.6.0",
    "aws-sdk": "^2.184.0",
    "bluebird": "^3.5.0",
    "cheerio": "^1.0.0-rc.2",
    "cheerio-tableparser": "^1.0.1",
    "date-fns": "^2.4.1",
    "he": "^1.1.1",
    "mongodb": "^3.0.5",
    "mz": "^2.7.0",
    "ramda": "^0.26.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.1",
    "source-map-support": "^0.5.13",
    "tough-cookie": "^2.3.4"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^0.1.3",
    "@types/async": "^3.0.2",
    "@types/aws-lambda": "^8.10.33",
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.2.3",
    "@types/cheerio": "^0.22.13",
    "@types/expect": "^1.20.4",
    "@types/he": "^1.1.0",
    "@types/mocha": "^5.2.7",
    "@types/mongodb": "^3.3.4",
    "@types/node": "^12.7.9",
    "@types/ramda": "^0.26.26",
    "@types/request-promise": "^4.1.44",
    "chai": "^4.1.2",
    "eslint": "^6.5.1",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-import": "^2.18.0",
    "husky": "^3.0.8",
    "mocha": "^6.2.0",
    "nodemon": "^1.11.0",
    "nyc": "^14.1.1",
    "prettier": "^1.18.2",
    "replace": "^1.1.1",
    "serverless-offline": "^5.7.1",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^6.2.0",
    "ts-mocha": "^6.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3",
    "webpack": "^4.41.0"
  }
}
