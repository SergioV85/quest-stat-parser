{
  "name": "quest-stat-parser-server",
  "version": "0.1.0",
  "description": "A server statistic parser for Quest.Ua games",
  "main": "server.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "scripts": {
    "compile": "tsc --declaration",
    "deploy": "serverless deploy",
    "debug": "SET SLS_DEBUG=* && node --inspect ./node_modules/serverless/bin/serverless offline -s dev",
    "test": "ts-mocha -p tsconfig.json src/**/*.test.ts",
    "test:coverage": "nyc ts-mocha -p tsconfig.json src/**/*.test.ts",
    "test:ci": "npm run test:coverage && npm run prepare-code-coverage-report",
    "start:local": "serverless offline",
    "start:server": "http-server",
    "prettier": "prettier --config ./.prettierrc --list-different \"./src/**/*.{ts,js,json}\"",
    "precommit": "npm run prettier && npm run lint",
    "lint": "eslint -c .eslintrc.js --ext .ts ./src",
    "prepare-code-coverage-report": "replace '...*src' 'SF:./src' coverage/lcov.info"
  },
  "author": "Serhii Vozniak",
  "license": "MIT",
  "dependencies": {
    "aws-lambda": "^1.0.5",
    "async": "^3.2.0",
    "aws-sdk": "~2.1354.0",
    "bluebird": "^3.7.0",
    "cheerio": "^1.0.0-rc.3",
    "cheerio-tableparser": "^1.0.1",
    "date-fns": "^2.14.0",
    "he": "^1.2.0",
    "mongodb": "~3.6.3",
    "mz": "^2.7.0",
    "ramda": "^0.27.0",
    "request": "^2.85.0",
    "request-promise": "^4.2.1",
    "source-map-support": "^0.5.13",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.0",
    "@types/async": "^3.2.0",
    "@types/aws-lambda": "^8.10.33",
    "@types/bluebird": "^3.5.27",
    "@types/chai": "^4.2.3",
    "@types/cheerio": "^0.22.13",
    "@types/expect": "^1.20.4",
    "@types/he": "^1.1.0",
    "@types/mocha": "^7.0.0",
    "@types/mongodb": "^3.5.0",
    "@types/node": "^12.7.9",
    "@types/ramda": "^0.27.7",
    "@types/request-promise": "^4.1.44",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/eslint-plugin-tslint": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "chai": "^4.1.2",
    "eslint": "^7.3.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsdoc": "^28.6.1",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.2.5",
    "mocha": "^8.0.0",
    "nodemon": "^1.11.0",
    "nyc": "^15.1.0",
    "prettier": "^2.0.5",
    "replace": "^1.1.1",
    "serverless": "^1.74.1",
    "serverless-offline": "^6.4.0",
    "serverless-plugin-typescript": "^1.1.9",
    "serverless-webpack": "^5.3.1",
    "ts-loader": "^7.0.0",
    "ts-mocha": "^7.0.0",
    "ts-node": "^8.4.1",
    "tslint": "^5.20.0",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.9.5",
    "webpack": "^4.41.0"
  }
}
